>>> t = ['a', 'b', 'c']
>>> x = t.pop(1)
>>> print(t)
 output = ['a', 'c']
>>> print(x)
output = b
>>> t = ['a', 'b', 'c']
>>> del t[1]
>>> print(t)
output = ['a', 'c']
>>> t = ['a', 'b', 'c']
>>> t.remove('b')
>>> print(t)
 output = ['a', 'c']
 >>> t = ['a', 'b', 'c', 'd', 'e', 'f']
>>> del t[1:5]
>>> print(t)
 output = ['a', 'f']



